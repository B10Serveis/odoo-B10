<odoo>
	<data>
		<record id="view_form_product_ccpae_inherited"
			model="ir.ui.view">
			<field name="name">Product ccpae</field>
			<field name="model">product.template</field>
			<field name="inherit_id"
				ref="product.product_template_form_view"/>
			<field name="arch"
				type="xml">
				<xpath expr="//div[@name='standard_price_uom']"
					position="after">
					<field name="ccpae"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
