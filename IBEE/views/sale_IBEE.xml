<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <record id="sale_IBEE_view_form" model="ir.ui.view">
            <field name="name">sale.form.IBEE</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/notebook/page/field[@name='order_line']/tree/field[@name='price_total']" position="before">
                    <field name="IBEE_sale"/>
                    <field name="PuntoVerde_sale"/>
                </xpath>                 
            </field>
        </record>        
    </data>
</odoo>