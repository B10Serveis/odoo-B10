<odoo>
  <data>
    <template id="model520_report_template">
      <t t-call="web.html_container">
        <t t-call="web.internal_layout">
          <div class="page">
            <div class="text-center">
              <h2>Model 520 - IBEE</h2>
            </div>
            <div>
            	<strong> Date Start: </strong><span t-esc="date_start" t-options="{'widget': 'date'}"/>
            	<strong> Date end: </strong><span t-esc="date_end" t-options="{'widget': 'date'}"/>
            	<strong> IBEE Total: </strong><span t-esc="IBEE8"/>
            </div>
            <span t-esc="batista10" />
            <table class="table table-condensed">
              <thead>
                <th>ID</th>
                <th>IBEE</th>
                <th>name</th>
              </thead>
              <tbody>
                <tr t-foreach="invoices_linies" t-as="lines">
                  <td>
                    <span t-esc="lines['ID']"/>
                  </td>
                  <td>
                    <span t-esc="lines['IBEE']"/>
                  </td>
                  <td>
                    <span t-esc="lines['name']"/>
                  </td>
                </tr>
              </tbody>
            </table>
          </div>
        </t>
      </t>
    </template>
  </data>
</odoo>
